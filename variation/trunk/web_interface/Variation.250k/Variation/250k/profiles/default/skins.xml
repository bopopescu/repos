<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="">

 <object name="Variation_250k_custom_images"
    meta_type="Filesystem Directory View"
    directory="Variation.250k:skins/Variation_250k_custom_images"/>
 <object name="Variation_250k_custom_templates"
    meta_type="Filesystem Directory View"
    directory="Variation.250k:skins/Variation_250k_custom_templates"/>
 <object name="Variation_250k_styles"
    meta_type="Filesystem Directory View"
    directory="Variation.250k:skins/Variation_250k_styles"/>

 <skin-path name="" based-on="Plone Default">
  <layer name="Variation_250k_custom_images"
     insert-after="custom"/>
  <layer name="Variation_250k_custom_templates"
     insert-after="Variation_250k_custom_images"/>
  <layer name="Variation_250k_styles"
     insert-after="Variation_250k_custom_templates"/>
 </skin-path>

</object>
